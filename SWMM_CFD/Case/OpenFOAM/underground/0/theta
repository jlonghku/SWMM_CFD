FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      theta;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.368;

boundaryField
{
    walls
    {
        type            calculated;
        value           uniform 0.368;
    }
    inlet1
    {
        type            calculated;
        value           uniform 0.368;
    }
    inlet2
    {
        type            calculated;
        value           uniform 0.368;
    }
    inlet3
    {
        type            calculated;
        value           uniform 0.368;
    }
}


