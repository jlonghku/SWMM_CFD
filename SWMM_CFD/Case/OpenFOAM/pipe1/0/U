FoamFile
{
    version         2;
    format          ascii;
    class           volVectorField;
    object          U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 1 -1 0 0 0 0 ];

internalField   uniform ( 0 0 0 );

boundaryField
{
    inlet
    {
        type            externalCoupledNew;
        commsDir        "/home/jlong/workspace/SWMM_CFD/Case/comms";
        waitInterval    0;
        calcInterval    30;
        file            data_OpenFOAM_pipe1_U;
        initByExternal  0;
    }
    walls
    {
        type            noSlip;
    }
    outlet
    {
        type            zeroGradient;
    }
}


